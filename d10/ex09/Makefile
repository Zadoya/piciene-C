NAME = ft_advanced_do-op
FLAGS = -Wall -Wextra -Werror

.PHONY: all clean

all: $(NAME)

$(NAME):
	gcc *c	$(FLAGS) -o $(NAME)

clean:
	@rm -f $(NAME)
